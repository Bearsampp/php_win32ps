  <filelist>
   <install as="dll/php_win32ps.dll" name="dll/php_win32ps-PHP_4_3.dll"/>
   <ignore name="dll/php_win32ps-PHP_4_4.dll"/>
   <ignore name="dll/php_win32ps-PHP_5_0.dll"/>
   <ignore name="dll/php_win32ps-PHP_5_1.dll"/>
  </filelist>
 </extbinrelease>
